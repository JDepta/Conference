<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="pl.edu.agh.ki.mwo.model.Reviewer" table="reviewers">
		<id name="authorId" column="reviewerId" type="long">
			<generator class="native"></generator>
		</id>
		
		<property name="name" column="name" type="string"></property>
		<property name="surname" column="surname" type="string"></property>
		<property name="email" column="email" type="string"></property>
		<property name="university" column="university" type="string"></property>
		
	</class>
</hibernate-mapping>