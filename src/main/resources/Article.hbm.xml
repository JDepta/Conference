<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="pl.edu.agh.ki.mwo.model.Article" table="articles">
		<id name="articleId" column="articleId" type="long">
			<generator class="native"></generator>
		</id>
		
		<property name="authorId" column="authorId" type="int"></property>
		<property name="title" column="title" type="string"></property>
		<property name="status" column="status" type="boolean"></property>
		<property name="score" column="score" type="double"></property>
		<property name="subjectId" column="subjectId" type="int"></property>

	</class>
</hibernate-mapping>